<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vlimv.taxi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".AccountTypeActivity" />
        <activity android:name=".DriverActivity" />
        <activity android:name=".PassengerActivity" />
        <activity android:name=".InvalidActivity" />
        <activity
            android:name=".DriverMainActivity"
            android:label="@string/title_activity_driver_main"
            android:theme="@style/AppTheme" />
    </application>

</manifest>